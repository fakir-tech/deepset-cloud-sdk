{
  "name": "deepset-cloud-sdk",
  "version": "0.0.1",
  "description": "Inoffficial for Deepset Cloud SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "which tsc",
    "build:module": "tsc -p tsconfig.module.json",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caki0915/my-awesome-greeter.git"
  },
  "keywords": [
    "deepset",
    "cloud",
    "nlp",
    "sdk"
  ],
  "author": "C-J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caki0915/my-awesome-greeter/issues"
  },
  "homepage": "https://github.com/caki0915/my-awesome-greeter#readme",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.191",
    "axios": "^1.3.2",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "p-limit": "^4.0.0"
  }
}
